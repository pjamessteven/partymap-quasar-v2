{
  "appId": "org.partymap.com",
  "appName": "PartyMap",
  "server": {
    "hostname": "api.partymap.com"
  },
  "bundledWebRuntime": false,
  "npmClient": "yarn",
  "webDir": "www",
  "plugins": {
    "CapacitorCookies": {
      "enabled": true
    },
    "CapacitorHttp": {
      "enabled": false
    },
    "SplashScreen": {
      "backgroundColor": "#282828"
    }
  },
  "ios": {
    "version": "1.3.0",
    "limitsNavigationsToAppBoundDomains": true
  },
  "android": {
    "versionCode": 7
  }
}